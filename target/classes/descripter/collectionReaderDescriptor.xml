<?xml version="1.0" encoding="UTF-8"?>
<collectionReaderDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <implementationName>FileSystemCollectionReader</implementationName>
  <processingResourceMetaData>
    <name>collectionReaderDescriptor</name>
    <description>It's a description of a Collection Reader.
The Input is the file route which would be used.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>InputDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>BackgroundKnowledgeFile_1</name>
